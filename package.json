{
  "name": "ourowallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "prettier": "prettier \"./src/**/*.+(js|jsx|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "lint:full": "npm run type-check && npm run lint",
    "validate": "npm run lint:full && npm run prettier -- --loglevel silent --list-different",
    "android": "react-native run-android",
    "a": "react-native run-android",
    "ios:se": "react-native run-ios --simulator='iPhone SE'",
    "ios:6s": "react-native run-ios --simulator='iPhone 6s'",
    "ios:8": "react-native run-ios --simulator='iPhone 8'",
    "ios": "react-native run-ios --simulator='iPhone Xs'",
    "start:rc": "npm start -- --reset-cache",
    "start": "react-native start",
    "test": "jest --watch",
    "test:prepush": "jest",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "reset": "watchman watch-del-all && rm -rf node_modules android/build android/app/build ios/build",
    "reset:hard": "npm run reset && rm -rf ios/Pods && rm -rf /tmp/metro-* && rm -rf ~/Library/Developer/Xcode/DerivedData"
  },
  "dependencies": {
    "@eva-design/eva": "^2.0.0-alpha.1",
    "@haskkor/react-native-recaptchav3": "^1.2.1",
    "@react-native-community/async-storage": "^1.10.1",
    "@react-native-community/clipboard": "^1.2.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.8.0",
    "@react-native-firebase/app": "^8.0.1",
    "@react-native-firebase/messaging": "^7.3.2",
    "@react-navigation/bottom-tabs": "^5.10.0",
    "@react-navigation/native": "^5.8.0",
    "@react-navigation/stack": "^5.10.0",
    "@sentry/react-native": "^1.4.5",
    "@types/airtable": "^0.8.0",
    "@ui-kitten/components": "^5.0.0-alpha.1",
    "@ui-kitten/eva-icons": "^5.0.0",
    "airtable": "^0.9.0",
    "axios": "^0.19.2",
    "d3-scale": "^3.2.1",
    "date-fns": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-react-native-wcandillon": "^2.9.3",
    "eslint-plugin-import": "^2.20.2",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "js-base64": "^2.5.2",
    "libphonenumber-js": "^1.7.53",
    "moment": "^2.25.3",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-chart-kit": "^5.5.0",
    "react-native-cli": "^2.0.1",
    "react-native-device-info": "^5.5.7",
    "react-native-fingerprint-scanner": "^6.0.0",
    "react-native-fix-image": "^2.0.1",
    "react-native-flash-message": "^0.1.15",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-haptic-feedback": "^1.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.4.0",
    "react-native-navigation-bar-color": "^2.0.1",
    "react-native-reanimated": "^1.10.1",
    "react-native-redash": "^13.6.0",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-svg": "^12.1.0",
    "react-native-webview": "^10.3.2",
    "rn-placeholder": "^3.0.1",
    "rn-secure-storage": "^2.0.4",
    "use-memo-one": "^1.1.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@types/axios": "^0.14.0",
    "@types/d3-scale": "^2.2.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^24.0.24",
    "@types/js-base64": "^2.3.1",
    "@types/node": "^13.13.5",
    "@types/react-native": "^0.62.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
